{
  "name": "observable-hooks",
  "version": "0.2.4",
  "description": "React hooks for RxJS Observables with powerful APIs.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/cjs/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:crimx/observable-hooks.git"
  },
  "author": "CRIMX<straybugs@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "shx rm -rf dist && yarn lint && yarn build:esm && yarn build:cjs",
    "build:esm": "shx rm -rf dist/esm && cross-env NODE_ENV=production tsc -p tsconfig.json --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "shx rm -rf dist/cjs && cross-env NODE_ENV=production tsc -p tsconfig.json --module commonjs --target es5 --outDir dist/cjs",
    "lint": "eslint src/**",
    "doc": "shx rm -rf typedoc && typedoc src",
    "test": "jest",
    "test:CI": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "rxjs": ">=6.0.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^1.1.0",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.8.2",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "coveralls": "^3.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.12.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rxjs": "^6.5.2",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  }
}
